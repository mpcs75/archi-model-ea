  <children
      xsi:type="archimate:SketchModelSticky"
      id="id-8b94b11726314b22a5484c8c891871d6"
      name="Sticky"
      font="1|Calibri|12.0|2|WINDOWS|1|-16|0|0|0|400|-1|0|0|0|3|2|1|34|Calibri"
      fontColor="#2e2e2e"
      lineColor="#000000"
      textAlignment="1"
